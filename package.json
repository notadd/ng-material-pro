{
  "name": "ng-material2",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build @notadd/ng-material2",
    "build:lib:prod": "ng build --prod @notadd/ng-material2",
    "publish:lib": "cd dist/notadd/ng-material2 && npm publish --access public",
