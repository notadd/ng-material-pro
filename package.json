{
  "name": "ng-material-pro",
  "version": "0.0.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build @notadd/ng-material-pro",
    "build:lib:prod": "ng build --prod @notadd/ng-material-pro",
    "publish:lib": "cd dist/notadd/ng-material-pro && npm publish --access public",
    "deploy": "ng build --prod --base-href /ng-material-pro/ && angular-cli-ghpages --dir ./dist/ng-material-pro/",
    "schematics:build": "tsc -p ./projects/schematics/tsconfig.json",
    "schematics:copy": "npm run schematics:build && webpack",
    "build-package": "npm run build:lib:prod && npm run schematics:copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.5",
    "@angular/cdk": "~8.0.0-rc.2",
    "@angular/common": "~8.0.0-rc.5",
    "@angular/compiler": "~8.0.0-rc.5",
    "@angular/core": "~8.0.0-rc.5",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0-rc.5",
    "@angular/material": "~8.0.0-rc.2",
    "@angular/platform-browser": "~8.0.0-rc.5",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.5",
    "@angular/router": "~8.0.0-rc.5",
    "core-js": "~3.0.1",
    "hammerjs": "~2.0.8",
    "rxjs": "~6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.4",
