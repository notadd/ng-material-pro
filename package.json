    "@angular/core": "^7.1.0-beta.2",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.1.0-beta.2",
    "@angular/http": "^7.1.0-beta.2",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "^7.1.0-beta.2",
    "@angular/platform-browser-dynamic": "^7.1.0-beta.2",
    "@angular/router": "^7.1.0-beta.2",
    "core-js": "^2.5.4",
