    "@types/node": "~10.12.6",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
